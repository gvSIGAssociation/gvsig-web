<html xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:dialog="urn:jsptagdir:/WEB-INF/tags/bootstrap/dialog/modal"
  xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"
  xmlns:tiles="http://tiles.apache.org/tags-tiles" xmlns:util="urn:jsptagdir:/WEB-INF/tags/util"
  xmlns:bootstrap-util="urn:jsptagdir:/WEB-INF/tags/bootstrap/util"
  xmlns:gvgeoportal-util="urn:jsptagdir:/WEB-INF/tags/gvgeoportal/util">

  <jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />

  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:directive.page pageEncoding="UTF-8" />

  <head>
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type" />
    <meta content="IE=8" http-equiv="X-UA-Compatible" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <bootstrap-util:load-styles-bootstrap />
    <util:load-scripts />
    <bootstrap-util:load-scripts-bootstrap />
    <gvgeoportal-util:load-styles />

    <title>${geoportal.titulo}</title>
  </head>

<body class="tundra spring" style="padding: 0px; padding-top: 49px;">

  <!-- Fixed navbar dropdown -->
  <header class="navbar navbar-inverse navbar-fixed-top bs-docs-nav">
    <div class="container">

      <div class="navbar-collapse collapse">
        <tiles:insertAttribute ignore="true" name="header" />
        <!--
        <tiles:insertAttribute ignore="true" name="menu" />
        -->
      </div>

    </div>
  </header>

  <div class="container-fluid">
    <div id="main">
      <dialog:message-box />
      <tiles:insertAttribute name="body" />
    </div>
  </div>

</body>
</html>
